# 1 
  a = int(input("Введите основание трапеции: "))
  b = int(input("Введите второе основание трапеции: "))
  c = int(input("Введите высоту трапеции: "))

  print("P =", a * b + 2 * c)
  
  #2 
  a = int(input("Введите объем тела: "))
  b = int(input("Введите массу тела: "))

  print("p =", b / a)
  
  #3
  a = int(input("Введите первое число: "))
  b = int(input("Введите второе число: "))

  print("Сумма =", a + b, "разность =", a - b, "произведение =", a * b, "частное =", a / b)
  
  #4
  
  from math import sqrt

  a = int(input("Введите первое положительное: "))
  b = int(input("Введите второе положительное: "))

  print("CA =", (a + b) / 2, "СГ =", sqrt(a * b))
  
  #5
  
  a, b = int(input("Введите площадь государства = ")), int(input("Введите кол-во жителей = "))

  print("Плотность населения =", b / a)
  
  #6
  
  a, b, c = int(input("Введите значение первого ребра = ")), int(input("Введите значение второго ребра = ")), int(input("Введите значение третьего ребра = "))

  print(("Объем прямоугольно параллелепипеда = "), a * b * c,'\n',("Площадь поверхности прямоугольно параллелепипеда = "), 2 * ((a * b) + (b * c) + (a * c)))
